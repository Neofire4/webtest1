{
    "name": "My Data Extractor",
    "version": "1.0.0",
    "description": "Extracts data from the parent page",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Data Extractor"
    },
    "content_scripts": [
        {
          "matches": ["http://127.0.0.1:8080/"],
          "js": ["content.js"]
        }
      ]
  }
  